html(lang="en")
  head
    title= title

    .container.blog_page
      .row.mb-5
        .col
          h1.blog_tittle #{ home.title }
          p #{ home.description+home.description+home.description+home.description }

      .row.d-flex.align-items-center.mb-5
        .col-md-6
          h1.blog_tittle #{ home.user_csm }
          p #{ home.user_csm_content }
        .col-md-6
          img.d-block.mx-lg-auto.img-fluid(src= '#{ home.user_csm_img }', alt='Bootstrap Themes', loading='lazy', width='700', height='500')

      .row.d-flex.align-items-center.mb-5.justify-content-between
        .col-md-5
            img.d-block.mx-lg-auto.img-fluid(src= '#{ home.user_csm_img_02 }', alt='Bootstrap Themes', loading='lazy', width='700', height='500')                 
        .col-md-6
          h1.blog_tittle #{ home.user_csm_02 }
          p #{ home.user_csm_content }
          ul.m-0.px-0
           each list in user_csm_02_ls_no 
            li #{ list.ls }
            
        